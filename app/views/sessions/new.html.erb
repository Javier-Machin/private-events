<div class="Signup_container">
  <div class="Signup_form">
    <%= image_tag "login1.jpg", class: "Image_login" %>
    <%= form_for(:session, url: login_path) do |f| %>
      <h2><%= f.label :email, 'User Email' %></h2>
      <%= f.text_field :email %>
      <h2><%= f.submit "Log in", class: "Submit_button" %></h2>
    <% end %>
  </div>
</div>
